<Window x:Class="NextCar.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NextCar"
        mc:Ignorable="d"
        Title="Login" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="122*"/>
            <RowDefinition Height="13*"/>
        </Grid.RowDefinitions>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="36,132,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="216" TextChanged="textBox_TextChanged" ToolTip="Username"/>
        <Button x:Name="button" Content="Login" HorizontalAlignment="Left" Height="29" Margin="185,186,0,0" VerticalAlignment="Top" Width="67" Click="button_Click"/>
        <Label x:Name="label" Content="Register" HorizontalAlignment="Left" Margin="199,220,0,0" VerticalAlignment="Top" Grid.RowSpan="2"/>
        <TextBox x:Name="passBox" HorizontalAlignment="Left" Height="23" Margin="36,158,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="216" TextChanged="textBox_TextChanged" ToolTip="Username"/>

    </Grid>
</Window>
